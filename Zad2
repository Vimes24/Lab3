public class lab3 {
    
    public static void main(String[] args) {

        // zadanie 2
       int[] tablica = {2, 6, 1};
       int max = 0;
        for (int i = 0; i < tablica.length; i++) {
            if (tablica[i] > max) {
                max = tablica[i];
            }
        }
        System.out.println("Max element to: " + max);
    }
    
}
